<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>

    <!--自定义seekbarstyle-->
    <style name="CustomSeekbarStyle">
        <item name="android:maxHeight">36px</item>
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/seekbar</item>
        <item name="android:thumb">@mipmap/font_seekbar_thumb</item>
        <item name="android:minHeight">36px</item>
    </style>
</resources>
